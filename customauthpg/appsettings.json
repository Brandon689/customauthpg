{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AdminConnection": "Host=localhost;Port=55432;Database=postgres;Username=postgres;Password=mysecretpassword",
    "DefaultConnection": "Host=localhost;Port=55432;Database=your_database_name;Username=your_app_user;Password=your_app_password"
  },
  "Database": {
    "Name": "your_database_name",
    "User": "your_app_user",
    "Password": "your_app_password"
  },
  "Jwt": {
    "Key": "your-256-bit-secret-key-here",
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "ExpirationInMinutes": 60
  }
}
